{
  "manifest_version": 3,
  "name": "Musiy Club & YouTube Playlist Exporter",
  "version": "1.0.7",
  "description": "Export Musify.club and Youtube.com (albums or playlist) to xspf playlists and stream in your favourite player.",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://musify.club/*",
    "https://www.youtube.com/*",
    "https://music.youtube.com/*",
    "https://invidious.snopyta.org/*",
    "https://yewtu.be/*",
    "https://invidious.kavin.rocks/*",
    "https://invidious.fdn.fr/*",
    "https://vid.puffyan.us/*",
    "https://invidious.tiekoetter.com/*",
    "https://inv.riverside.rocks/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://musify.club/*",
        "https://www.youtube.com/*",
        "https://music.youtube.com/*"
      ],
      "js": [
        "utils.js",
        "content_scripts.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}